<h1>Listing Studnets</h1>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>
  <tbody>
  <% @students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.email %></td>
      <td><%= link_to 'View', student %></td>
      <td><%= link_to 'Edit', edit_student_path(student) %></td>
    </tr>
    <% end %>
  </tbody>
  </table></td>